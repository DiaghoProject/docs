{
    "openapi": "3.0.2",
    "info": {
        "title": "Diagho Interpretation",
        "description": "The Diagho Interpretation API",
        "version": "0.1.0"
    },
    "paths": {
        "/api/v1/": {
            "get": {
                "tags": ["Information"],
                "summary": "Information",
                "description": "Returns some information about the current application.",
                "operationId": "root_api_v1__get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Root Api V1  Get",
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/species/homo_sapiens/assemblies/": {
            "get": {
                "tags": ["Assemblies"],
                "summary": "Homo Sapiens Assembly List",
                "description": "Returns the list of available assemblies for homo sapiens.",
                "operationId": "homo_sapiens_assembly_list_api_v1_species_homo_sapiens_assemblies__get",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Homo Sapiens Assembly List Api V1 Species Homo Sapiens Assemblies  Get",
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/species/homo_sapiens/assemblies/{assembly}/": {
            "get": {
                "tags": ["Assemblies"],
                "summary": "Homo Sapiens Assembly Detail",
                "description": "Returns information about a specific assembly for homo sapiens.",
                "operationId": "homo_sapiens_assembly_detail_api_v1_species_homo_sapiens_assemblies__assembly___get",
                "parameters": [{
                    "description": "Pick your favorite assembly.",
                    "required": true,
                    "schema": {
                        "allOf": [{
                            "$ref": "#/components/schemas/Assembly"
                        }],
                        "description": "Pick your favorite assembly."
                    },
                    "name": "assembly",
                    "in": "path"
                }],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "title": "Response Homo Sapiens Assembly Detail Api V1 Species Homo Sapiens Assemblies  Assembly   Get",
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Assembly": {
                "title": "Assembly",
                "enum": ["grch37p13", "grch38p13"],
                "type": "string"
            },
            "Variant": {
                "title": "Variant",
                "required": ["chrom", "pos", "ref", "alt", "assembly"],
                "type": "object",
                "properties": {
                    "chrom": {
                        "title": "Chrom",
                        "type": "string",
                        "example": "20"
                    },
                    "pos": {
                        "title": "Pos",
                        "type": "integer",
                        "example": 1234567
                    },
                    "id": {
                        "title": "Id",
                        "type": "string",
                        "default": ".",
                        "example": "."
                    },
                    "ref": {
                        "title": "Ref",
                        "type": "string",
                        "example": "A"
                    },
                    "alt": {
                        "title": "Alt",
                        "type": "string",
                        "example": "C"
                    },
                    "assembly": {
                        "allOf": [{
                            "$ref": "#/components/schemas/Assembly"
                        }],
                        "description": "Reference genome."
                    }
                }
            },
            "ValidationError": {
                "title": "ValidationError",
                "required": ["loc", "msg", "type"],
                "type": "object",
                "properties": {
                    "loc": {
                        "title": "Location",
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "msg": {
                        "title": "Message",
                        "type": "string"
                    },
                    "type": {
                        "title": "Error Type",
                        "type": "string"
                    }
                }
            },
            "HTTPValidationError": {
                "title": "HTTPValidationError",
                "type": "object",
                "properties": {
                    "detail": {
                        "title": "Detail",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        }
                    }
                }
            }
        }
    }
}
